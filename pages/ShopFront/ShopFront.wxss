/* 商品描述评价 */
.evaluate{
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  padding: 10rpx 24rpx;
  box-sizing: border-box;
  font-size: 22rpx;
  line-height: 40rpx;
}
.bottomEval:nth-child(1) .pingjia{
  background-color: rgba(231,38,10,0.2);
  color: #E7260A;
}
.bottomEval:nth-child(2) .pingjia{
  background-color: rgba(245,175,25,0.2);
  color: #F5AF19;
}
.bottomEval:nth-child(3) .pingjia{
  background-color: rgba(102,102,102,0.2);
  color: #E7260A;
}
.bottomEval:nth-child(1) .pingjia,.bottomEval:nth-child(2) .pingjia,.bottomEval:nth-child(3) .pingjia{
  vertical-align: text-bottom;
  font-size: 16rpx;
  display: inline-block;
  width: 40rpx;
  height: 24rpx;
  line-height: 24rpx;
  text-align: center;
  border-radius: 20rpx;
  margin-left: 10rpx;
}
