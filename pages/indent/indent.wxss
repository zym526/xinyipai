page{
  background-color: #ececec;
}
/* 上导航 */
.topDH{
  background-color: white;
  font-size: 30rpx;
  line-height: 70rpx;
  display: flex;
  justify-content: space-between;
  padding: 15rpx 24rpx;
  margin-bottom: 24rpx;
}
.success{
  color: #E7260A;
  font-size: 22rpx;
}
