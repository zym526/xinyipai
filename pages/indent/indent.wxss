.redText{
  color: #E7260A;
  border-bottom: 4rpx solid #E7260A;
}
.rightDH::before{
  content: "";
  display: inline-block;
  width: 2rpx;
  height: 40rpx;
  position: absolute;
  top: 30rpx;
  left: 90rpx;
  background-color: #c6c6c6;
}
/* 商品 */
.van-cell{
  padding: 24rpx!important;
}
.van-cell:after{
  content: none!important;
}
.bottomOne{
  background-color: white;
  margin-bottom: 24rpx;
}
.outerAll{
  padding: 0rpx 24rpx 24rpx;
}
.outerImg{
  width: 150rpx;
  height: 150rpx;
  background-color: #e0e0e0;
  border-radius: 10rpx;
  text-align: center;
}
.outerImg image{
  width: 96rpx!important;
  height: 96rpx!important;
  margin-top: 13.5rpx;
}
.topText{
  width: 70%;
  white-space:normal;
  font-size: 28rpx;
  ext-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.bottomText{
  font-size: 22rpx;
  color: #333333;
}
/* 删除/查看物流/评价 */
.bottomThree{
  padding: 0rpx 24rpx 24rpx;
  display: flex;
  justify-content: space-between;
  position: relative;
}
.btnAll{
  display: inline-block;
}
.bottomThree button{
  display: inline-block;
  width: 127.4rpx!important;
  font-size: 24rpx;
  line-height: 45.6rpx;
  padding: 0rpx;
  font-weight: 300;
  background-color: rgba(0,0,0,0);
}
.leftBtn{
  border: 1rpx solid #666666;
  border-radius: 45.6rpx;
  color: #666666;
  margin-right: 11rpx!important;
}
.rightBtn{
  border: 1rpx solid #FF0000;
  border-radius: 45.6rpx;
  color: #FF0000;
}
/* 删除订单 */
.invite {
  width: 230rpx;
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding-left: 24rpx;
  position: absolute;
  background-color: white;
  border-radius: 10rpx;
  left: 24rpx;
  top: 60rpx;
  box-shadow: 0rpx 0rpx 10rpx rgba(0,0,0,0.1);
}
.invite:after {
  content: '';
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  top: -11rpx;
  left: 20rpx;
  transform: rotate(45deg);
  background-color: white;
  box-shadow: -5rpx -5rpx 5rpx rgba(0,0,0,0.03);
}
/* 没有订单 */
.bottomTopImg{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bottomTopImg image:first-child{
  margin-top: 91rpx;
}
.bottomTopImg image:last-child{
  margin-top: 6rpx;
  margin-bottom: 88rpx;
}
/* 为您推荐 */
.centerTJ{
  text-align: center;
  font-size: 30rpx;
  margin-bottom: 25rpx;
}
.centerTJ text{
  margin: 0rpx 15rpx;
}
.centerTJ image{
  width: 28rpx;
  height: auto;
  vertical-align: -3rpx;
}
/* 推荐商品 */
.paipinOuter{
  width: 100%;
  padding: 0rpx 24rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
}
.paipin{
  width: calc(50% - 12rpx);
  background-color: white;
  border-radius: 10rpx;
  margin-bottom: 24rpx;
}
.paipin:nth-child(2n-1){
  margin-right: 24rpx;
}
.paipinImg{
  width: 100%;
  height: 340rpx;
  background-color: #D2D2D2;
  text-align: center;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
}
.paipinImg image{
  width: 215rpx;
  height: auto;
  margin-top: 62.5rpx;
}
.paipinTitle{
  font-size: 28rpx;
  letter-spacing: 0;
  overflow: hidden;
  display: -webkit-box;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;  /*要显示的行数*/
  -webkit-box-orient: vertical;
  padding: 0rpx 12rpx;
}
.paipinTitle2{
  padding: 0rpx 12rpx 12rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.dangqianjia{
  font-size: 26rpx;
  color: #666666;
}
.jiage{
  font-size: 30rpx;
  color: #E7260A;
}
.zhaoxiangsi{
  width: 65rpx;
  height: 25rpx;
  border: 1rpx solid #666666;
  color: #666666;
  font-size: 16rpx;
  line-height: 25rpx;
  text-align: center;
  border-radius: 25rpx;
}